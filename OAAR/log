probCreate( scip, probName, Topology, Flow, nFlow )
struct SCIP_ProbData
{
   SCIP_VAR**            vars;         /**< all exiting variables in the problem */
   SCIP_CONS**           conss;        /**< all constraints */
   OAARTopology*         Topology;     /**< Topology of the problem */
   OAARFlow**            Flow;         /**< Flow array storing all user defined flow infomation */ 
   int                   nFLow;        /**< Number of flows */
   int                   nvars;        /**< number of generated variables */
   int                   varssize;     /**< size of the variable array */
};

